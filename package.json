{
  "name": "@buidlerlabs/memejob-sdk-js",
  "version": "0.0.1",
  "description": "The official memejob.fun development kit",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/buidler-labs/memejob-sdk-js.git"
  },
  "homepage": "https://memejob.fun",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "pnpm clean:build && tsup --config tsup.config.ts",
    "clean": "rimraf ./dist ./node_modules",
    "clean:build": "rimraf dist",
    "check": "biome check --write",
    "dev": "tsup --config tsup.config.ts --watch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "format": "biome format --write",
    "postinstall": "node ./scripts/generate-api-schema.mjs",
    "test": "vitest"
  },
  "keywords": [
    "sdk",
    "memejob",
    "memejob.fun",
    "memejob sdk",
    "memecoins",
    "launchpad",
    "development kit"
  ],
  "author": "Buidlerlabs",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "node": "./dist/index.js"
    },
    "./abi": {
      "import": "./dist/abi/index.mjs",
      "node": "./dist/abi/index.js"
    },
    "./adapters": {
      "import": "./dist/adapters/index.mjs",
      "node": "./dist/adapters/index.js"
    },
    "./chains": {
      "import": "./dist/chains/index.mjs",
      "node": "./dist/chains/index.js"
    },
    "./ipfs": {
      "import": "./dist/ipfs/index.mjs",
      "node": "./dist/ipfs/index.js"
    },
    "./mirror": {
      "import": "./dist/mirror/index.mjs",
      "node": "./dist/mirror/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "node": "./dist/utils/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "abi": [
        "dist/abi/index.d.ts"
      ],
      "adapters": [
        "dist/adapters/index.d.ts"
      ],
      "chains": [
        "dist/chains/index.d.ts"
      ],
      "ipfs": [
        "dist/ipfs/index.d.ts"
      ],
      "mirror": [
        "dist/mirror/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@hashgraph/sdk": "^2.66.0",
    "@types/node": "^24.0.1",
    "chalk": "^5.4.1",
    "dotenv": "^16.5.0",
    "meow": "^13.2.0",
    "msw": "^2.10.2",
    "openapi-typescript": "7.5.2",
    "openapi-typescript-fetch": "^2.1.0",
    "openapi-typescript-helpers": "^0.0.15",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "viem": "^2.31.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.3",
    "vue": "^3.5.16"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "multiformats": "^13.3.7",
    "openapi-fetch": "^0.13.4",
    "openapi-typescript-helpers": "^0.0.15"
  },
  "peerDependencies": {
    "@hashgraph/sdk": "^2.66.0",
    "viem": "^2.31.3"
  }
}
